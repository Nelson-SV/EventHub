            </Label>
            <Label fx:id="custEmailLB" prefWidth="250.0" text="Customer Email">
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="73.0">
         <children>
            <VBox prefHeight="100.0" prefWidth="63.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="39.0" prefWidth="73.0">
                     <children>
                        <Label fx:id="extraLabel2" prefHeight="12.0" prefWidth="50.0" rotate="90.0" text="Ticket Price">
                           <font>
                              <Font size="6.0" />
                           </font>
                        </Label>
                        <Label fx:id="extraLabel1" prefHeight="13.0" prefWidth="50.0" rotate="90.0" text="Ticket Type">
