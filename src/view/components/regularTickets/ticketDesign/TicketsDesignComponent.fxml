            <ImageView fx:id="logoImg" fitHeight="100.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" rotate="90.0" />
            <ImageView fx:id="barCode" fitHeight="100.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" rotate="90.0" />
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="213.0">
         <children>
            <Label fx:id="eventNameLB" prefWidth="250.0" text="Event Name Line">
               <VBox.margin>
                  <Insets bottom="10.0" left="15.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="eventDateLB" prefWidth="250.0" text="Event Date Line">
               <VBox.margin>
                  <Insets bottom="10.0" left="15.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="eventLocationLB" prefWidth="250.0" text="Event Location Line">
