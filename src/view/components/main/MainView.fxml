<?import javafx.scene.text.Font?>

<StackPane fx:id="mainLayout" minHeight="360.0" minWidth="640.0" prefHeight="720.0" prefWidth="1280.0" styleClass="mainParent" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.components.main.MainController">
    <children>
        <VBox alignment="TOP_CENTER" prefHeight="720.0" prefWidth="1280.0">
            <children>
                <HBox fx:id="navigation" alignment="CENTER_LEFT" prefHeight="60.0" prefWidth="1280.0" styleClass="mainNavigation">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="70.0" prefWidth="120.0" styleClass="eventLogo" text="EventHub">
                            <font>
                                <Font name="System Bold Italic" size="24.0" />
                            </font>
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Label>
                        <MFXButton fx:id="eventsNavButton" alignment="CENTER" prefHeight="60.0" prefWidth="120.0" rippleColor="#102c57" styleClass="controlNav" text="Events">
                            <font>
                                <Font name="Arial Bold" size="18.0" />
                            </font>
                        </MFXButton>

                              <MFXButton fx:id="sellingNavButton" alignment="CENTER" prefHeight="60.0" prefWidth="120.0" rippleColor="#102c57" styleClass="controlNav" text="Selling">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </MFXButton>
                        <MFXButton fx:id="ticketingNavButton" alignment="CENTER" onAction="#createTicket" prefHeight="60.0" prefWidth="120.0" rippleColor="#102c57" styleClass="controlNav" text="Ticketing">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </MFXButton>
                    </children>
                </HBox>
                <HBox prefHeight="10.0" prefWidth="1280.0" styleClass="transitionBar" VBox.vgrow="NEVER">
                    <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" height="10.0" smooth="false" strokeType="INSIDE" styleClass="eventBar" width="120.0" />
                        <Rectangle fx:id="eventsLine" arcHeight="5.0" arcWidth="5.0" height="10.0" smooth="false" strokeType="INSIDE" styleClass="eventBar" width="120.0" />
                        <Rectangle fx:id="sellingLine" arcHeight="5.0" arcWidth="5.0" height="10.0" smooth="false" strokeType="INSIDE" styleClass="eventBar" width="120.0" />
                        <Rectangle fx:id="ticketingLine" arcHeight="5.0" arcWidth="5.0" height="10.0" smooth="false" strokeType="INSIDE" styleClass="eventBar" width="120.0" />
                    </children>
                </HBox>
                <GridPane alignment="CENTER" maxHeight="80.0" maxWidth="1072.0" prefHeight="80.0" prefWidth="1072.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label contentDisplay="CENTER" maxHeight="30.0" maxWidth="104.0" prefHeight="30.0" prefWidth="104.0" text="Events" textFill="#102c57" GridPane.halignment="LEFT" GridPane.valignment="BOTTOM">
                            <font>
                                <Font name="System Bold" size="29.0" />
                            </font>
                            <GridPane.margin>
                                <Insets />
                            </GridPane.margin>
                        </Label>
                        <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="30.0" maxWidth="156.0" mnemonicParsing="false" onAction="#createEvent" prefHeight="30.0" prefWidth="156.0" styleClass="createEvent" text="Create event" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.valignment="BOTTOM">
                            <font>
                                <Font name="System Bold" size="16.0" />
                            </font>
                        </Button>
                    </children>
                </GridPane>
                <GridPane maxHeight="30.0" maxWidth="1072.0" prefHeight="30.0" prefWidth="1072.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <TextField maxHeight="30.0" maxWidth="312.0" prefHeight="30.0" prefWidth="312.0" promptText="Search events..." styleClass="searchBox" />
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" />
                    </VBox.margin>
                </GridPane>
            <HBox fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="30.0" minWidth="536.0" prefHeight="30.0" prefWidth="1076.0" styleClass="eventsHeader">
               <children>
                  <Label alignment="CENTER" maxHeight="30.0" maxWidth="520.0" minHeight="30.0" minWidth="280.0" prefHeight="30.0" prefWidth="520.0" text="Event">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" maxHeight="30.0" maxWidth="112.0" minHeight="30.0" minWidth="60.0" prefHeight="30.0" prefWidth="112.0" text="Status" />
                  <Label alignment="CENTER" maxHeight="30.0" maxWidth="112.0" minHeight="30.0" minWidth="60.0" prefHeight="30.0" prefWidth="112.0" text="Tickets" />
                  <Label alignment="CENTER" maxHeight="30.0" maxWidth="112.0" minHeight="30.0" minWidth="60.0" prefHeight="30.0" prefWidth="112.0" text="Start Date" />
                  <Label alignment="CENTER" maxHeight="30.0" maxWidth="212.0" minHeight="30.0" minWidth="106.0" prefHeight="30.0" prefWidth="212.0" text="Actions" />
               </children>
            </HBox>
            <ScrollPane fx:id="eventsView" hbarPolicy="NEVER" maxHeight="485.0" maxWidth="1076.0" minHeight="242.0" minWidth="536.0" prefHeight="485.0" prefWidth="1076.0" styleClass="eventsView" VBox.vgrow="NEVER">
               <content>
                  <VBox fx:id="mainEventContainer" alignment="TOP_CENTER" fillWidth="false" spacing="5.0" />
               </content>
            </ScrollPane>
            </children>
            <StackPane.margin>
                <Insets />
            </StackPane.margin>
        </VBox>
        <StackPane fx:id="secondaryLayout" minHeight="360.0" minWidth="640.0" prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: rgba(0, 0, 0, 0.5);" visible="false">
         <padding>
            <Insets bottom="70.0" left="102.0" right="102.0" top="70.0" />
         </padding></StackPane>

    </children>
</StackPane>
